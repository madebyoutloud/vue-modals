{
  "name": "@outloud/vue-modals",
  "title": "Vue Modals",
  "description": "The smart modal library for Vue.js",
  "author": "Outloud (https://outloud.co)",
  "contributors": [
    "Andrej Adamcik"
  ],
  "private": false,
  "version": "2.0.2",
  "license": "MIT",
  "type": "module",
  "repository": {
    "type": "git",
    "url": "https://github.com/madebyoutloud/vue-modals"
  },
  "keywords": [
    "modal",
    "modals",
    "vue",
    "popup"
  ],
  "main": "./dist/index.cjs",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.mjs",
      "require": "./dist/index.cjs",
      "types": "./dist/index.d.ts"
    },
    "./style.css": {
      "import": "./dist/style.css",
      "require": "./dist/style.css"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "dev": "vue-tsc --noEmit && vite build -w",
    "build": "vite build",
    "test": "vitest run",
    "typecheck": "vue-tsc --noEmit",
    "lint:fix": "eslint --fix .",
    "prepack": "pnpm build",
    "release": "release-it"
  },
  "dependencies": {
    "@vueuse/core": ">=9",
    "tua-body-scroll-lock": "^1.5.0"
  },
  "peerDependencies": {
    "vue": "^3"
  },
  "devDependencies": {
    "@vitejs/plugin-vue": "^5.0.4",
    "@vueuse/core": "^10",
    "sass": "^1.77.1",
    "tua-body-scroll-lock": "^1.5.0",
    "vite": "^5.2.11",
    "vite-plugin-dts": "^3.9.1",
    "vitest": "^1.6.0",
    "vue": "^3.4",
    "vue-tsc": "^2.0.17"
  }
}
