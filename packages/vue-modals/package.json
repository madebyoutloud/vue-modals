{
  "name": "@outloud/vue-modals",
  "description": "Modal library for vue 3.",
  "private": false,
  "version": "1.0.2",
  "source": "src/index.ts",
  "main": "./dist/index.umd.js",
  "module": "./dist/index.es.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.es.js",
      "require": "./dist/index.umd.js",
      "types": "./dist/index.d.ts"
    },
    "./style.css": {
      "import": "./dist/style.css",
      "require": "./dist/style.css"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "dev": "vue-tsc --noEmit && vite build -w",
    "build": "vue-tsc --noEmit && vite build",
    "test": "vitest run",
    "typecheck": "vue-tsc --noEmit",
    "release": "pnpm build && release-it"
  },
  "dependencies": {
    "@vueuse/core": "^9.11.1",
    "@vueuse/integrations": "^9.11.1",
    "focus-trap": "^7.2.0"
  },
  "devDependencies": {
    "@release-it/conventional-changelog": "^5.1.1",
    "@vitest/coverage-istanbul": "^0.28.1",
    "@vue-macros/volar": "^0.5.15",
    "jsdom": "^21.1.0",
    "release-it": "^15.6.0",
    "unplugin-vue-define-options": "^1.1.4",
    "unplugin-vue-macros": "^1.4.2",
    "vitest": "^0.28.1"
  },
  "peerDependencies": {
    "@vueuse/core": ">=9.11.1",
    "@vueuse/integrations": ">=9.11.1",
    "focus-trap": ">=7.2.0",
    "vue": "^3"
  }
}
