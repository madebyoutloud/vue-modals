{
  "github": {
    "release": true
  },
  "git": {
    "tagName": "v${version}",
    "requireCleanWorkingDir": false
  },
  "npm": {
    "publish": false
  },
  "plugins": {
    "@release-it/conventional-changelog": {
      "preset": "angular",
      "infile": "CHANGELOG.md",
      "ignoreRecommendedBump": true
    }
  }
}
